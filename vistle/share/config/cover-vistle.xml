    <VncClient value="on" rfbHost="viscose.hlrs.de" >
    </VncClient>
    -->

    <AKToolbar value="on">
     <Attachment value="TOP" />
     <Scale value=".8" />
     <MinClickTime value="0.2" />
     <Position x="-980" y="0" z="980" />
